<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>   
    <class name="jsf.jpa.Session" table="lo54_session">
        <id name="id" column="id">
            <generator class="sequence">
     			<param name="sequence">SESSION_ID_SEQ</param>
			</generator>
        </id>        
        <property name="start" column="start" />
        <property name="end" column="end" />
        <property name="locationId" column="location_id" />
        <property name="courseCode" column="course_code" />
    </class>
</hibernate-mapping>

